				play CHOICE_FOCUS_SOUND;																																\
				execnow set selectedPlayerXuid ( getfeederdata( "xuid" ) );																								\
				execnow set selectedFriendName ( getfeederdata( "name" ) );																								\
				if( (getfeederdata( "xuid" ) != getXuid()) && (getfeederdata( "xuid" ) != 0) )																			\
