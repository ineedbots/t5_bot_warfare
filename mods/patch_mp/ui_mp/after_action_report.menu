			textaligny		paligny																	\
			textstyle		pstyle																	\
			textfont		pfont																	\
			textscale		psize																	\
			forecolor		pcolor																	\
			exp				text( ptext );															\
			visible			pvis																	\
			action																					\
			{																						\
				if( pageNumber == 1 )																\
				{																					\
					closeImmediate menu_aar_summary;												\
				}																					\
				elseif( pageNumber == 2 )															\
				{																					\
					if( dvarInt(ui_aar_current_view_num) == AAR_VIEW_AWARDS_PERSONAL_BESTS )		\
