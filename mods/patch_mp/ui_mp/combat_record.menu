		ITEM_ANIMATE( menuName, "info_bar_1", slide, SPEED ); \
		ITEM_ANIMATE( menuName, "info_bar", slide, SPEED ); \
		ITEM_ANIMATE( menuName, "info_bar_5", slideAndFadeOut, SPEED ); \
		ITEM_ANIMATE( menuName, "scroll_bar", slide, SPEED ); \
