#define DOWN_ACTION( menuName ) \
		if( dvarbool("ui_combatLockScrolling") == 0 ) \
		{ \
			if( canScrollUpOrDown( 0 ) && DOES_MENU_HAVE_SCROLLING_ENABLED ) \
			{ \
				SCROLL_DOWN_ACTION( menuName ) \
			} \
		} 

#define UP_ACTION( menuName ) \
		if( dvarbool("ui_combatLockScrolling") == 0 ) \
		{ \
			if( canScrollUpOrDown( 1 ) ) \
			{ \
