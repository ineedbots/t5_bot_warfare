	}

	execKeyInt DPAD_UP
	{
		execnow set fsSelectedFileID ( getFeederData( "fileShareSlots", "fileID" ) );
		execnow set fshSelectLastSlotRow 1;
		setfocus fileShareSlots;
	}

	execKeyInt APAD_DOWN
	{
