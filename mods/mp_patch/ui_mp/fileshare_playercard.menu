		rect			((-SCREENSHOT_VIEWER_WIDTH/2)+420) (SCREENSHOT_VIEWER_HEIGHT/2+4) 0 20 HORIZONTAL_ALIGN_CENTER VERTICAL_ALIGN_CENTER
		exp				rect X( ZOOM_TEXT_X )
		text			"@PATCH_SCREENSHOT_ZOOM" 
		textfont		CHOICE_TEXTFONT  
